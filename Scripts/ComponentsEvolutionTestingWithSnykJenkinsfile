                            echo "=============== $\{tool\} VERSION DEFAULT ==================="\
                     try \{\
                               \
                           sh "sudo -su aicha.war /usr/local/bin/snyk code test /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{index\} --detection-depth=3"\
                        \} catch (err) \{\
                           \
