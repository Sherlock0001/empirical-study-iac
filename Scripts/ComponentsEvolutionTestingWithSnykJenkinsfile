import groovy.json.JsonSlurperClassic\
\
pipeline\{\
\
   agent any\
   \
  \
   stages\{\
     \
\
    stage('Clone tools source codes') \{\
      steps \{\
             script \{\
                   sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk"\
                   sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk"\
                   for (tool in TOOLS)\{\
                        def index = TOOLS.findIndexOf \{ it == tool \}\
                        def i = 2*index\
                        def TOOLs = (tool=="puppet") ? "puppetlabs": (tool=="salt") ? "saltstack": (tool=="terraform" || tool=="vagrant") ? "hashicorp" : tool\
    \
                          sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{index\}"\
                          sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{index\}"\
                          sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{VERSIONS[i]\}"\
                          sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{VERSIONS[i]\}"\
                          sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{VERSIONS[i+1]\}"\
                          sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{VERSIONS[i+1]\}"\
                           \
                           try \{\
                              sh "git clone https://github.com/$\{TOOLs\}/$\{tool\}.git /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{index\}"\
                           \} catch (err) \{\
                               echo "something failed"\
                           \}\
                           try \{\
                              sh "git clone -b $\{VERSIONS[i]\} --depth 1 https://github.com/$\{TOOLs\}/$\{tool\}.git /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{VERSIONS[i]\}"\
                           \} catch (err) \{\
                               echo "something failed"\
                           \}\
                           try \{\
                              sh "git clone -b $\{VERSIONS[i+1]\} --depth 1 https://github.com/$\{TOOLs\}/$\{tool\}.git /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/$\{tool\}/$\{VERSIONS[i+1]\}"\
                           \} catch (err) \{\
                               echo "something failed"\
                           \}\
                   \}\
                \}\
            \}\
       \}\
       \
    stage('Clone scripts source codes') \{\
      steps \{\
             script \{\
                   for (script in SCRIPTS)\{\
                        def index = SCRIPTS.findIndexOf \{ it == script \}\
                        def i = 2*index\
                       \
                        def scriptFolder1 = SCRIPTS[index].split('/')\
                        def scriptFolder2 = scriptFolder1[scriptFolder1.size() -1].minus('.git')\
                        \
                          sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{index\}"\
                          sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{index\}"\
                          sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{SCRIPTSVERSIONS[i]\}"\
                          sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{SCRIPTSVERSIONS[i]\}"\
                          sh "mkdir -p /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{SCRIPTSVERSIONS[i+1]\}"\
                          sh "sudo -su aicha.war chmod 777  /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{SCRIPTSVERSIONS[i+1]\}"\
                           \
                           try \{\
                              sh "git clone $\{script\} /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{index\}"\
                           \} catch (err) \{\
                               echo "something failed"\
                           \}\
                           try \{\
                              sh "git clone -b $\{SCRIPTSVERSIONS[i]\} --depth 1 $\{script\} /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{SCRIPTSVERSIONS[i]\}"\
                           \} catch (err) \{\
                               echo "something failed"\
                           \}\
                           try \{\
                              sh "git clone -b $\{SCRIPTSVERSIONS[i+1]\} --depth 1 $\{script\} /Users/aicha.war/.jenkins/workspace/componentsevotestingsnyk/scripts/$\{scriptFolder2\}/$\{SCRIPTSVERSIONS[i+1]\}"\
                           \} catch (err) \{\
                               echo "something failed"\
                           \}\
                   \}\
                \}\
            \}\
       \}\
